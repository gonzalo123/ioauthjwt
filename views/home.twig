<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
<ul>
    <li><a href="/login">login</a></li>
    <li><a href="/private">private page</a></li>
</ul>

<script src="http://localhost:3000/socket.io/socket.io.js"></script>

<script>
    // we try to connect to websocket from a page without sessionID
    var socket = io('http://localhost:3000/', {query: 'token=notValidToken'});
    // this event never is triggered
    socket.on('connect', function () {
        console.log("connected!");
    });
    // websocket doesn't receive any data
    socket.on('hello', function (data) {
        console.log(data);
    });

    socket.on('error', function(err) {
        console.log(err);
    })
</script>

</body>
</html>